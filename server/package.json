{
  "name": "eclypse-server",
  "version": "1.0.0",
  "description": "Backend server for Eclypse e-commerce website",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm install @types/node @types/express @types/cors --save-dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "openai": "^4.103.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}
